# Environment Variables Template
# Copy this to .env for local development

# AWS Backend
VITE_API_BASE_URL=https://YOUR_API.execute-api.us-east-1.amazonaws.com/dev1
VITE_CLOUDFRONT_DOMAIN=YOUR_CLOUDFRONT_DOMAIN
VITE_AWS_REGION=us-east-1
VITE_ENVIRONMENT=dev1

# Stripe (use test key for development)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_YOUR_KEY

# Google Maps
VITE_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_KEY

# Firebase Configuration
VITE_FIREBASE_API_KEY=YOUR_FIREBASE_KEY
VITE_ENABLE_FIREBASE_CHAT=true
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=YOUR_SENDER_ID
VITE_FIREBASE_APP_ID=YOUR_APP_ID
VITE_FIREBASE_MEASUREMENT_ID=YOUR_MEASUREMENT_ID


